#include <stdio.h>
#include <string.h>

int lengthOfLastWord(char *s) {
    int len = strlen(s);
    int count = 0;

    // Step 1: Skip trailing spaces
    int i = len - 1;
    while (i >= 0 && s[i] == ' ') {
        i--;
    }

    // Step 2: Count last word length
    while (i >= 0 && s[i] != ' ') {
        count++;
        i--;
    }

    return count;
}
